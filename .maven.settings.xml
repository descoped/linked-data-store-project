<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0">

    <!-- add this profile to your ~/.m2/settings.xml file -->
    <profiles>
        <profile>
            <id>cantara-releases</id>
            <repositories>
                <repository>
                    <id>cantara-releases</id>
                    <name>Cantara Releases</name>
                    <url>https://mvnrepo.cantara.no/content/repositories/</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>never</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>never</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>

</settings>
